<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.loan555.roomdemo">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RoomDemo">
        <activity android:name="com.loan555.roomdemo.ui.activities.UpdateActivity"></activity>
        <activity
            android:name="com.loan555.roomdemo.ui.activities.AddNoteActivity"
            android:parentActivityName="com.loan555.roomdemo.ui.activities.MainActivity" />
        <activity android:name="com.loan555.roomdemo.ui.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>